.swatch-mesh {
  background-image: url('{{ "mesh.jpg" | asset_url }}');
}

.swatch-lace {
  background-image: url('{{ "lace.jpg" | asset_url }}');
}

/* ==========================================================================
   # @font-face
   ========================================================================== */


 
 // h1 - h4
 @font-face {
   font-family: "GTAmerica-ExpandedBold";
   src: url("{{ 'GTAmerica-ExpandedBold_2.eot' | asset_url }}");
   src: url("{{ 'GTAmerica-ExpandedBold_2.eot?#iefix' | asset_url }}")
       format("embedded-opentype"),
     url("{{ 'GTAmerica-ExpandedBold_2.woff2' | asset_url }}") format("woff2"),
     url("{{ 'GTAmerica-ExpandedBold_2.woff' | asset_url }}") format("woff"),
     url("{{ 'GTAmerica-ExpandedBold_2.ttf' | asset_url }}") format("truetype"),
     url("{{ 'GTAmerica-ExpandedBold_2.svg#GTAmerica-ExpandedBold' | asset_url }}")
       format("svg");
   font-weight: 400;
   font-style: normal;
 }
 
 // h5, p, p.caption
 @font-face {
   font-family: "GTAmerica-Regular";
